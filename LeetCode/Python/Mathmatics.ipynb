   "metadata": {},
   "outputs": [],
   "source": [
    "from math import factorial\n",
    "\n",
    "def getRow(rowIndex):\n",
    "    rst = []\n",
    "    num = factorial(rowIndex)\n",
    "    for i in range(rowIndex+1):\n",
    "        rst.append(num//(factorial(i) * factorial(rowIndex-i)))\n",
    "        \n",
    "    return rst"
   ]
